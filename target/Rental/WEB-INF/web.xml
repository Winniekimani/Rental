<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Winnie</servlet-name>
    <servlet-class>com.winnie.servlet.Hello</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>WinnieAgain</servlet-name>
    <servlet-class>com.winnie.servlet.HelloAgain</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>WinnieFinal</servlet-name>
    <servlet-class>com.winnie.servlet.HelloFinal</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Winnie</servlet-name>
    <url-pattern>/jonathan</url-pattern>

  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>WinnieAgain</servlet-name>
    <url-pattern>/keziah</url-pattern>

  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>WinnieFinal</servlet-name>
    <url-pattern>/ivy</url-pattern>

  </servlet-mapping>

</web-app>

